{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "indent": ["error", 2]
    }
}
